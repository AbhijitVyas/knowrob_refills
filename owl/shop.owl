<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY shop "http://knowrob.org/kb/shop.owl#" >
    <!ENTITY swrl "http://www.w3.org/2003/11/swrl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY knowrob "http://knowrob.org/kb/knowrob.owl#" >
    <!ENTITY computable "http://knowrob.org/kb/computable.owl#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://knowrob.org/kb/shop.owl#"
     xml:base="http://knowrob.org/kb/shop.owl"
     xmlns:shop="http://knowrob.org/kb/shop.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:computable="http://knowrob.org/kb/computable.owl#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:knowrob="http://knowrob.org/kb/knowrob.owl#">
    <owl:Ontology rdf:about="http://knowrob.org/kb/shop.owl">
        <owl:imports rdf:resource="package://knowrob_assembly/owl/knowrob_assembly.owl"/>
        <owl:imports rdf:resource="package://knowrob_common/owl/knowrob.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://knowrob.org/kb/shop.owl#adjacentMountingBar -->

    <owl:ObjectProperty rdf:about="&shop;adjacentMountingBar">
        <rdfs:range rdf:resource="&shop;ShelfMountingBar"/>
        <rdfs:domain rdf:resource="&shop;ShelfMountingBar"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#adjacentProductFacing -->

    <owl:ObjectProperty rdf:about="&shop;adjacentProductFacing">
        <rdfs:comment>Adjacent facings do share one separator. Each facing is adjacent to itself (due to chain axiom).</rdfs:comment>
        <rdfs:domain rdf:resource="&shop;ProductFacing"/>
        <rdfs:range rdf:resource="&shop;ProductFacing"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&shop;separatorOfFacing"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="&shop;separatorOfFacing"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#articleNumber -->

    <owl:ObjectProperty rdf:about="&shop;articleNumber">
        <rdfs:range rdf:resource="&shop;ArticleNumber"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#articleNumberOfFacing -->

    <owl:ObjectProperty rdf:about="&shop;articleNumberOfFacing">
        <rdfs:range rdf:resource="&shop;ArticleNumber"/>
        <rdfs:domain rdf:resource="&shop;ProductFacing"/>
        <rdfs:subPropertyOf rdf:resource="&shop;articleNumber"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#articleNumberOfLabel -->

    <owl:ObjectProperty rdf:about="&shop;articleNumberOfLabel">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&shop;ArticleNumber"/>
        <rdfs:domain rdf:resource="&shop;ShelfLabel"/>
        <rdfs:subPropertyOf rdf:resource="&shop;articleNumber"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#articleNumberOfProduct -->

    <owl:ObjectProperty rdf:about="&shop;articleNumberOfProduct">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment>SKUs are unique for products (functional), and products may only have one SKU (inverse functional)</rdfs:comment>
        <rdfs:range rdf:resource="&shop;ArticleNumber"/>
        <rdfs:domain rdf:resource="&shop;Product"/>
        <rdfs:subPropertyOf rdf:resource="&shop;articleNumber"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#barcodeOfProduct -->

    <owl:ObjectProperty rdf:about="&shop;barcodeOfProduct">
        <rdfs:domain rdf:resource="&shop;Product"/>
        <rdfs:range rdf:resource="&shop;ShelfLabel"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&shop;articleNumberOfProduct"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="&shop;articleNumberOfLabel"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#belowFacing -->

    <owl:ObjectProperty rdf:about="&shop;belowFacing">
        <rdfs:domain rdf:resource="&knowrob;SpatialThing-Localized"/>
        <rdfs:subPropertyOf rdf:resource="&knowrob;locatedAtRelative"/>
        <rdfs:range rdf:resource="&shop;ProductFacing"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#currentActivity -->

    <owl:ObjectProperty rdf:about="&shop;currentActivity"/>
    


    <!-- http://knowrob.org/kb/shop.owl#emptySpaceOfShelf -->

    <owl:ObjectProperty rdf:about="&shop;emptySpaceOfShelf"/>
    


    <!-- http://knowrob.org/kb/shop.owl#labelOfFacing -->

    <owl:ObjectProperty rdf:about="&shop;labelOfFacing">
        <rdfs:domain rdf:resource="&shop;ProductFacing"/>
        <rdfs:range rdf:resource="&shop;ShelfLabel"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#layerOfFacing -->

    <owl:ObjectProperty rdf:about="&shop;layerOfFacing"/>
    


    <!-- http://knowrob.org/kb/shop.owl#leftOfProductFacing -->

    <owl:ObjectProperty rdf:about="&shop;leftOfProductFacing">
        <rdfs:subPropertyOf rdf:resource="&shop;adjacentProductFacing"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&shop;leftSeparator"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="&shop;rightSeparator"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#leftSeparator -->

    <owl:ObjectProperty rdf:about="&shop;leftSeparator">
        <rdfs:subPropertyOf rdf:resource="&shop;separatorOfFacing"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#mountingBarOfFacing -->

    <owl:ObjectProperty rdf:about="&shop;mountingBarOfFacing">
        <rdfs:domain rdf:resource="&shop;ProductFacing"/>
        <rdfs:range rdf:resource="&shop;ShelfMountingBar"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#nextMountingBar -->

    <owl:ObjectProperty rdf:about="&shop;nextMountingBar">
        <rdfs:range rdf:resource="&shop;ShelfMountingBar"/>
        <rdfs:domain rdf:resource="&shop;ShelfMountingBar"/>
        <rdfs:subPropertyOf rdf:resource="&shop;adjacentMountingBar"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#nextShelfAffordance -->

    <owl:ObjectProperty rdf:about="&shop;nextShelfAffordance">
        <rdfs:domain rdf:resource="&shop;ShelfLayerSlideInF"/>
        <rdfs:range rdf:resource="&shop;ShelfLayerSlideInF"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#previousMountingBar -->

    <owl:ObjectProperty rdf:about="&shop;previousMountingBar">
        <rdfs:domain rdf:resource="&shop;ShelfMountingBar"/>
        <rdfs:range rdf:resource="&shop;ShelfMountingBar"/>
        <rdfs:subPropertyOf rdf:resource="&shop;adjacentMountingBar"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#productInFacing -->

    <owl:ObjectProperty rdf:about="&shop;productInFacing">
        <rdfs:comment>yields all objects that are part of some facing</rdfs:comment>
        <rdfs:range rdf:resource="&shop;Product"/>
        <rdfs:domain rdf:resource="&shop;ProductFacing"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#rightOfProductFacing -->

    <owl:ObjectProperty rdf:about="&shop;rightOfProductFacing">
        <rdfs:subPropertyOf rdf:resource="&shop;adjacentProductFacing"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&shop;rightSeparator"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="&shop;leftSeparator"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#rightSeparator -->

    <owl:ObjectProperty rdf:about="&shop;rightSeparator">
        <rdfs:subPropertyOf rdf:resource="&shop;separatorOfFacing"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#separatorOfFacing -->

    <owl:ObjectProperty rdf:about="&shop;separatorOfFacing">
        <rdfs:domain rdf:resource="&shop;ProductFacing"/>
        <rdfs:range rdf:resource="&shop;ShelfSeparator"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#siblingProductInFacing -->

    <owl:ObjectProperty rdf:about="&shop;siblingProductInFacing">
        <rdfs:domain rdf:resource="&shop;Product"/>
        <rdfs:range rdf:resource="&shop;Product"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&shop;validFacingForProduct"/>
            <rdf:Description rdf:about="&shop;productInFacing"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#targetGroupOfProduct -->

    <owl:ObjectProperty rdf:about="&shop;targetGroupOfProduct">
        <rdfs:domain rdf:resource="&shop;Product"/>
        <rdfs:range rdf:resource="&shop;ShopPersona"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#validFacingForProduct -->

    <owl:ObjectProperty rdf:about="&shop;validFacingForProduct">
        <rdfs:comment>yields all facings associated with the sku of some product</rdfs:comment>
        <rdfs:domain rdf:resource="&shop;Product"/>
        <rdfs:range rdf:resource="&shop;ProductFacing"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&shop;articleNumberOfProduct"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="&shop;articleNumberOfFacing"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://knowrob.org/kb/shop.owl#ean -->

    <owl:DatatypeProperty rdf:about="&shop;ean">
        <rdfs:domain rdf:resource="&shop;EuropeanArticleNumber"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&shop;widthOfProduct">
        <rdfs:range rdf:resource="&shop;Product"/>
        <rdfs:subPropertyOf rdf:resource="&knowrob;widthOfObject"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&shop;heightOfProduct">
        <rdfs:range rdf:resource="&shop;Product"/>
        <rdfs:subPropertyOf rdf:resource="&knowrob;heightOfObject"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&shop;depthOfProduct">
        <rdfs:range rdf:resource="&shop;Product"/>
        <rdfs:subPropertyOf rdf:resource="&knowrob;depthOfObject"/>
    </owl:DatatypeProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#isSpaceRemainingInFacing -->

    <owl:DatatypeProperty rdf:about="&shop;isSpaceRemainingInFacing"/>
    


    <!-- http://knowrob.org/kb/shop.owl#priceOfProduct -->

    <owl:DatatypeProperty rdf:about="&shop;priceOfProduct"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://knowrob.org/kb/computable.owl#PrologTemporalProperty -->

    <owl:Class rdf:about="&computable;PrologTemporalProperty"/>
    


    <!-- http://knowrob.org/kb/shop.owl#ArticleNumber -->

    <owl:Class rdf:about="&shop;ArticleNumber">
        <rdfs:subClassOf rdf:resource="&knowrob;InformationBearingThing"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#BackRoomOfShop -->

    <owl:Class rdf:about="&shop;BackRoomOfShop">
        <rdfs:subClassOf rdf:resource="&shop;ShopRoom"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&knowrob;typePrimaryFunction-StoragePlaceFor"/>
                <owl:someValuesFrom rdf:resource="&shop;Product"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#BottomLayerConnectedToShelf -->

    <owl:Class rdf:about="&shop;BottomLayerConnectedToShelf">
        <rdfs:subClassOf rdf:resource="&shop;ShelfConnection"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://knowrob.org/kb/knowrob_assembly.owl#consumesAffordance"/>
                <owl:onClass rdf:resource="&shop;ShelfBottomLayerAffordanceM"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://knowrob.org/kb/knowrob_assembly.owl#consumesAffordance"/>
                <owl:onClass rdf:resource="&shop;ShelfBottomLayerAffordanceF"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#Customer -->

    <owl:Class rdf:about="&shop;Customer">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&knowrob;Person"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&shop;currentActivity"/>
                        <owl:someValuesFrom rdf:resource="&shop;Shopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&shop;ShopPersona"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#EmptyProductFacing -->

    <owl:Class rdf:about="&shop;EmptyProductFacing">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&shop;ProductFacing"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&shop;articleNumberOfFacing"/>
                        <owl:onClass rdf:resource="&shop;ArticleNumber"/>
                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&shop;productInFacing"/>
                        <owl:onClass rdf:resource="&shop;Product"/>
                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&shop;ProductFacing"/>
        <rdfs:comment rdf:datatype="&xsd;string">Facing with associated SKU, but without any product item.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#EmptyShelfSpace -->

    <owl:Class rdf:about="&shop;EmptyShelfSpace">
        <rdfs:subClassOf rdf:resource="&shop;ShelfSpace"/>
        <rdfs:comment rdf:datatype="&xsd;string">Shelf space that is not assigned to a specific product and can be used for additional facings of other products or new products. See also “Free Shelf Space”, “Facing”</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#EuropeanArticleNumber -->

    <owl:Class rdf:about="&shop;EuropeanArticleNumber">
        <rdfs:subClassOf rdf:resource="&shop;ArticleNumber"/>
        <rdfs:comment>short: EAN</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#FemaleCustomer -->

    <owl:Class rdf:about="&shop;FemaleCustomer">
        <rdfs:subClassOf rdf:resource="&shop;Customer"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#FreeProductFacing -->

    <owl:Class rdf:about="&shop;FreeProductFacing">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&shop;ProductFacing"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&shop;articleNumberOfFacing"/>
                        <owl:onClass rdf:resource="&shop;ArticleNumber"/>
                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&shop;isSpaceRemainingInFacing"/>
                        <owl:hasValue rdf:datatype="&xsd;boolean">true</owl:hasValue>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&shop;ProductFacing"/>
        <knowrob:todo>compute `isSpaceRemainingInFacing`</knowrob:todo>
        <rdfs:comment>Facing that has a SKU associated, and has some space left for at least one other item of that SKU. Note: `skuOfFacing` can be computed based on barcode locations, and `isSpaceRemainingInFacing` based how many items are in this facing, and what their size is.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#FreeShelfSpace -->

    <owl:Class rdf:about="&shop;FreeShelfSpace">
        <rdfs:subClassOf rdf:resource="&shop;ShelfSpace"/>
        <rdfs:comment rdf:datatype="&xsd;string">Shelf space that is assigned to a specific product. It can either be an empty facing or available shelf capacity behind products on the shelf. See also “Empty Shelf Space”, “Empty Facing”, “Facing”</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#FullProductFacing -->

    <owl:Class rdf:about="&shop;FullProductFacing">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&shop;ProductFacing"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&shop;articleNumberOfFacing"/>
                        <owl:onClass rdf:resource="&shop;ArticleNumber"/>
                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&shop;isSpaceRemainingInFacing"/>
                        <owl:hasValue rdf:datatype="&xsd;boolean">false</owl:hasValue>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&shop;ProductFacing"/>
        <rdfs:comment>Facing with associated SKU and without space for any additional items. Note: `skuOfFacing` is computed based on location of barcodes, and `isSpaceRemainingInFacing` based on number of objects in the facing and their size,</rdfs:comment>
        <knowrob:todo>compute `isSpaceRemainingInFacing`</knowrob:todo>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#FullShelfSpace -->

    <owl:Class rdf:about="&shop;FullShelfSpace">
        <rdfs:subClassOf rdf:resource="&shop;ShelfSpace"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#HangInAffordance -->

    <owl:Class rdf:about="&shop;HangInAffordance">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/knowrob_assembly.owl#AssemblyAffordance"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#HangingProductOnShelf -->

    <owl:Class rdf:about="&shop;HangingProductOnShelf">
        <rdfs:subClassOf rdf:resource="&shop;ShopActivity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&knowrob;goalLocation"/>
                <owl:onClass rdf:resource="&shop;ShelfMountingBar"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&knowrob;objectActedOn"/>
                <owl:onClass rdf:resource="&shop;MountableProduct"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#LabelConnectedToShelf -->

    <owl:Class rdf:about="&shop;LabelConnectedToShelf">
        <rdfs:subClassOf rdf:resource="&shop;ShelfConnection"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://knowrob.org/kb/knowrob_assembly.owl#consumesAffordance"/>
                <owl:onClass rdf:resource="&shop;ShelfLabelSlideInM"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://knowrob.org/kb/knowrob_assembly.owl#needsAffordance"/>
                <owl:allValuesFrom rdf:resource="&shop;ShelfLabelAffordance"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://knowrob.org/kb/knowrob_assembly.owl#needsAffordance"/>
                <owl:onClass rdf:resource="&shop;ShelfLabelSlideInF"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#LargeVolumeShelf -->

    <owl:Class rdf:about="&shop;LargeVolumeShelf">
        <rdfs:subClassOf rdf:resource="&shop;ShelfSystem"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#LayerConnectedToShelf -->

    <owl:Class rdf:about="&shop;LayerConnectedToShelf">
        <rdfs:subClassOf rdf:resource="&shop;ShelfConnection"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://knowrob.org/kb/knowrob_assembly.owl#consumesAffordance"/>
                <owl:allValuesFrom rdf:resource="&shop;ShelfLayerAffordance"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#MaleCustomor -->

    <owl:Class rdf:about="&shop;MaleCustomor">
        <rdfs:subClassOf rdf:resource="&shop;Customer"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#MisplacedProduct -->

    <owl:Class rdf:about="&shop;MisplacedProduct">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&shop;PlacedProduct"/>
                    <owl:Class>
                        <owl:complementOf rdf:resource="&shop;WellPlacedProduct"/>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&shop;PlacedProduct"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/shop.owl#MisplacedProductFacing -->

    <owl:Class rdf:about="&shop;MisplacedProductFacing">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&shop;ProductFacing"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&shop;productInFacing"/>
                        <owl:someValuesFrom rdf:resource="&shop;MisplacedProduct"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
